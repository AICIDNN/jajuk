<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<shortcuts>
   <programGroup defaultName='Jajuk' location='applications'/>
  <shortcut
     name='Jajuk'
     programGroup='yes'
     desktop='yes'
     applications='no'
     startMenu='no'
     startup='no'
     target='javaw'
     commandLine='-client -Xms25M -Xss5M -Xmx40M -Djava.library.path=..\native -jar "$INSTALL_PATH/bin/jajuk.jar" -notaskbar'
     workingDirectory='$INSTALL_PATH/bin'
     description='Jajuk advanced JukeBox'
     iconIndex='0'
     iconFile='$INSTALL_PATH/dist-files/images/jajuk-logo.ico'
     initialState='noShow'>
     <createForPack name='main pack'/>
   </shortcut>
    <shortcut
     name='Uninstall'
     programGroup='yes'
     desktop='no'
     applications='no'
     startMenu='no'
     startup='no'
     target='javaw'
     commandLine='-jar "$INSTALL_PATH/Uninstaller/uninstaller.jar"'
     workingDirectory='$INSTALL_PATH/Uninstaller'
     description='Uninstall Jajuk'
     iconIndex='0'
     iconFile='$INSTALL_PATH/dist-files/images/jajuk-uninstall.ico'
     initialState='noShow'>
     <createForPack name='main pack'/>
   </shortcut>
  </shortcuts>
